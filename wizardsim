while true do
    for _, npc in ipairs(npcs) do
        local args = {
            [1] = 1,
            [2] = npc
        }
        game:GetService("ReplicatedStorage").Remote.CastSpell:FireServer(unpack(args))
        wait(0.3) -- Delay between each spell
    end
end
